{
    "manifest_version": 2,
    "name": "Cookie Closer",
    "short_name": "cookcl",
    "description": "Test",
    "version": "1.3",
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Test"
    },
    "background": {
      "scripts": [
        "./src/js/background/background.js"
      ]
    },
    "permissions": [
      "activeTab",
      "tabs",
      "contentSettings"
    ],
    "content_scripts": [
      {
        "js": [
          "./src/js/content/content.js"
        ],
        "matches": [
          "http://*/*",
          "https://*/*"
        ]
      }
    ]
  }